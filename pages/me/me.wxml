<!--pages/me/me.wxml-->
<view class="user-area">
 <view class="user-row">
    <image class="user-img" src="{{userImg}}" />
    <text class="user-name">{{userName}}</text>
 </view>
  <text class="active-code">激活码次数{{num}}</text>
</view>
<input class="active-input"   placeholder="激活码" />
<button class="use-active-code-btn">使用激活码</button>
<button class="contact-btn" open-type="contact" bindcontact="handleContact">联系客服</button>
